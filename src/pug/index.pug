include ./mixins/mixins
doctype html
html(lang="en")
  include ./partials/header
  body
    +title('Spells')
    .card-list
      each item in spells
        +spell(item)

    +title('Rules')
    .card-list
      each item in rules
        +rule(item)

    +title('Tables')
    .card-list
      include ./partials/ranges

      
